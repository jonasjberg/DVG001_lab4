jonas@debian:~$ 
for ea in router,193 m1,10 m2,20                                                            
do
    enhet="${ea%,*}"; adress="${ea#*,}"
    printf "\n%s\n" "ENHET: ${enhet}"
    ipcalc "192.168.133.${adress}/25"
done

